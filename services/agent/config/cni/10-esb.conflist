{
  "cniVersion": "1.0.0",
  "name": "esb-net",
  "plugins": [
    {
      "type": "bridge",
      "bridge": "esb0",
      "isGateway": true,
      "ipMasq": true,
      "dns": {
        "nameservers": ["10.88.0.1"]
      },
      "ipam": {
        "type": "host-local",
        "subnet": "10.88.0.0/16",
        "routes": [{ "dst": "0.0.0.0/0" }]
      }
    },
    {
      "type": "portmap",
      "capabilities": {
        "portMappings": true
      }
    }
  ]
}
