# E2Eテスト用環境変数 (Containerd)
# run_tests.py によって読み込まれます
# 標準シナリオ設定をベースに、ネットワーク設定のみオーバーライドします

# オートスケーリング設定 (テスト用)
DEFAULT_MAX_CAPACITY=3
GATEWAY_IDLE_TIMEOUT_SECONDS=15
ORPHAN_GRACE_PERIOD_SECONDS=30
HEARTBEAT_INTERVAL=5

# コンテナ管理設定
ENABLE_CONTAINER_PAUSE=false

# 既存ネットワークとの競合を避けるためのサブネット変更
RUNTIME_NET_SUBNET=172.26.0.0/16
RUNTIME_NODE_IP=172.26.0.10
VHOST_VSOCK_REQUIRED=0
DEVMAPPER_POOL=
